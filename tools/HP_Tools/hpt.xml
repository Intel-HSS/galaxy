<tool id="hpt" name="Application">
  <description></description>
  <command interpreter="python">/opt/HarmonyPoint/Galaxy/galaxy-dist/tools/HP_Tools/submitter/submitter.py $input1 $input2</command>
  <inputs>
    <param name="input1" type="select" label="Select Application">
       <option value="bcftool">bcftool</option>
       <option value="model_creation">model_creation</option>
       <option value="survival_prediction">survival_prediction</option>
       <option value="ga4gh">ga4gh</option>
